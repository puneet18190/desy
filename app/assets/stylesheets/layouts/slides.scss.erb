      line-height: 32px;
      
    }
    
    .size7 {
      
      font-size: 35px;
      line-height: 39px;
      
    }
    
  }
  
  &.audio {
    
    .title-container {
      
      height: 110px;
      margin-bottom: 20px;
      
    }
    
    .audio-container {
      
      height: 71px;
      margin-bottom: 20px;
      
    }
    
    .text-container {
      
      height: 329px;
      
    }
    
  }
  
  &.cover {
    
    width: 900px;
    height: 590px;
    padding: 0;
    
    .image-container {
      
      height: 560px;
      
      .image {
        
        &.not-editable {
          
          width: 900px;
          height: 560px;
          overflow: hidden;
          position: relative;
          
          img {
            
            position: absolute;
            
          }
          
        }
        
        &.editable {
          
          .mask {
            
            width: 900px;
            height: 560px;
            overflow: hidden;
            
            .alignable {
              
              position: relative;
              
              img {
                
                cursor: url("https://mail.google.com/mail/images/2/openhand.cur"), move;
                
              }
              
            }
            
            .add {
              
              top: 215px;
              opacity: 0.8;
              
            }
            
          }
          
          .empty-mask {
            
            width: 900px;
            height: 560px;
            text-align: center;
            color: #999999;
            background-color: #f1f1f1;
            font-size: 16px;
            
            p {
              
              margin-top: 280px;
              
            }
            
          }
          
        }
        
      }
      
      .add {
        
        @include photo-icon;
        cursor: pointer;
        position: absolute;
        top: 140px;
        left: 385px;
        
        &:hover {
          
          @include plus-icon;
          
        }
        
      }
      
    }
    
    .title-container {
      
      height: 110px;
      width: 880px;
      top: 430px;
      position: absolute;
      padding-top: 20px;
      padding-left: 20px;
      font-family: "Nunito", Helvetica, sans-serif;
      background-color: rgba(242,148,0,0.95);
      color: #fff;
      font-size: 50px;
      font-weight: normal;
      line-height: 50px;
      
    }
    
    .footer-container {
      
      background-color: white;
      color: #373737;
      height: 16px;
      width: 870px;
      padding: 7px 15px;
      font-size: 13px;
      font-family: Arial, sans-serif;
      
      .subject {
        
        float: left;
        
      }
      
      .author {
        
        float: right;
        
        .name {
          
          color: #F29400;
          
        }
        
      }
      
    }
    
  }
  
  &.image1 {
    
    .title-container {
      
      height: 110px;
      margin-bottom: 20px;
      
    }
    
    .image-container {
      
      width: 420px;
      height: 420px;
      margin-right: 20px;
      
    }
    
    .text-container {
      
      width: 420px;
      height: 420px;
      
    }
    
  }
  
  &.image2 {
    
    .image-container {
      
      width: 420px;
      margin-right: 20px;
      
    }
    
  }
  
  &.image3 {
    
    .image-container {
      
    }
    
  }
  
  &.image4 {
    
    .image-container {
      
      width: 420px;
      height: 265px;
      margin-right: 20px;
      margin-bottom: 20px;
      
    }
    
  }
  
  &.text {
    
    .title-container {
      
