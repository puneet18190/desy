@mixin photo-icon {
  
  font-size: 130px;
  background: url('/assets/icone_piene.svg') no-repeat scroll -2em center / 6em 1em transparent;
  width: 130px;
  height: 130px;
  
}

@mixin plus-icon {
  
  background-position: -3em center;
  
}

.slide-content {
  
  height: 550px;
  width: 860px;
  padding: 20px;
  font-family: Arial,Verdana,sans-serif;
  background-color: white;
  -webkit-box-shadow: 0px 6px 16px #161515;
  box-shadow: 0px 6px 16px #161515;
  
  .item {
    
    float: left;
    
  }
  
  .visible {
    
    display: block;
    
  }
  
  .hidden {
    
    display: none;
    
  }
  
  .text-container {
    
