html.prelogin-layout {
   height: 100%;
  
  body {
    font: 80% Arial, Helvetica, sans-serif;
	  background: #0f0f0f;
	  margin: 0;
	  color: #fff;
	  text-align: center;
		width: 100%;
		height: 100%;
  }
	.top {
    margin-bottom: -190px;
    background: transparent;
    width: 1%;
    height: 50%;
    float: left;
  }
	.container {
    margin: 0 auto;
    width: 1024px;
    background-color: #030303;
		border:1px solid red;
    clear: left;
		position: relative;
	}
	.header {
	  background-image:url(<%= asset_path 'bg_prelogin.jpg' %>);
    background-repeat:no-repeat;
		background-position: top center;
		padding-bottom:398px;
		ul {
		  border:1px solid red;
		  padding:0;
		  margin:0
		}
		li {
		  display: inline;
      list-style-type: none;
      padding-right: 20px;
		}
	}
}
