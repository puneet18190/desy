html.audio_editor-controller {
  
  #audio_editor {
    
    .audioComponentContent {
      float: left;
      width: 700px;
      height: 100px;
      background-color: aqua;
      color: blue;
      margin: 5px;
    }
    
    .manigliaAudio {
      float: left;
      width: 100px;
      height: 100px;
      background-color: maroon;
      margin: 5px;
    }
    
    .contatore {
      float: left;
      margin: 5px;
    }
    
    .audioComponentDurations {
      width: 260px;
      height: 20px;
      float: left;
      background-color: purple;
    }
    
    .audioComponentBottom {
      width: 700px;
      height: 20px;
      background-color: black;
    }
    
    .audioComponentTitle {
      width: 700px;
      height: 20px;
      background-color: yellow;
    }
    
    .audioComponentControls {
      width: 200px;
      height: 20px;
      float: right;
      background-color: green;
    }
    
    .audioComponentPlayer {
      width: 700px;
      height: 60px;
      background-color: red;
    }
    
  }
  
}
