$('#lesson_editor_image_gallery_container ._image_gallery_thumb').css('margin-bottom', '0');
<% @images.each do |image| %>
  $('#lesson_editor_image_gallery_container #image_gallery_content .jspPane').append('<%= j(render(:partial => "galleries/thumbs/image", :locals => {:image => image, :index => 7})) %>');
<% end %>
$('#lesson_editor_image_gallery_container').data('page', <%= @page %>);
$('._select_image_from_gallery').addClass('_add_image_to_slide');
