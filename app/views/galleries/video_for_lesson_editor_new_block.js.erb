<% @videos.each do |video| %>
  $('#lesson_editor_video_gallery_container #video_gallery_content .jspPane').append('<%= j(render(:partial => "galleries/thumbs/video", :locals => {:video => video})) %>');
<% end %>
$('#lesson_editor_video_gallery_container').data('page', <%= @page %>);
$('._select_video_from_gallery').addClass('_add_video_to_slide');
