<ul id="image_gallery_content" class="scrollVerde">
  <div class="scroll-pane">
    <% if images.empty? %>
    <div class="emptyGalleryImageEditor">
      <p><%= t('galleries.empty.image') %></p>
    </div>
    <% else %>
      <% images.each do |image| %>
        <%= render :partial => "galleries/thumbs/image", :locals => {:image => image} %>
      <% end %>
    <% end %>
  </div>
</ul>
