<ul id="image_gallery_content" class="scrollVerde">
  <div class="scroll-pane">
    <% images.each do |image| %>
      <li id="gallery_image_<%= image.id %>" class="_image_gallery_thumb" data-image-id="<%= image.id %>">
        <%= link_to (image_tag image.thumb, :width => '111'), "#{image.id}" %>
        <%= render :partial => "galleries/popups/image", :locals => {:image => image} %>
      </li>
    <% end %>
  </div>
</ul>
