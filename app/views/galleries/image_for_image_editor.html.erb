<%= render :partial => 'image_editor/header', :locals => {:image => nil} %>
<div id="image_gallery_for_image_editor" data-page="1" data-tot-pages="<%= @tot_pages %>">
  <div class="mediaEditorElementContainer">
    <%= render :partial => 'galleries/image', :locals => {:images => @images} %>
  </div>
</div>

<% if @images.count > 21 %>
  <script type="text/javascript" charset="utf-8">
    initializeImageGalleryInImageEditor();
  </script>
<% end %>
