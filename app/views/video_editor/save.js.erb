<% if @redirect %>
  
  window.location = '<%= dashboard_path %>'
  
<% else %>
  
  <% if @errors %>
    $('#error_messages').html('<%= j standard_form_error_messages(@errors) %>');
    <% @error_fields.each do |err|%>
      $('<%= @error_ids %>_<%= err %>').addClass('form_error');
    <% end %>
  <% else %>
    window.location = '<%= my_media_elements_path %>'
  <% end %>
  
<% end %>
