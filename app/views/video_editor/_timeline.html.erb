<div class="scrollVerde">
  <div class="scroll-pane horizontal-only" id="media_elements_list_in_video_editor">
    <div class="scrollContent">
      
      <% cont = 1 %>
      <% parameters[:parameters].each do |p| %>
        <div class="lista">
          <%= render :partial => "video_editor/components/#{p[:component]}", :locals => {:component => p, :position => cont} %>
          <div class="separatore"><%= image_tag("arrow_element_timeline.png") %></div>
        </div>
        <% cont += 1 %>
      <% end %>
      
      <%= render :partial => "new_element" %>
      
    </div>
  </div>
</div>
