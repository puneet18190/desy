<%
  div_id = empty ? '' : "id=\"video_component_#{identifier}_cutter\""
  duration = empty ? 0 : component[:duration]
%>
<div class="contentCutterTextImage _video_component_cutter" style="display:none" <%= div_id.html_safe %>>
  <div class="cutter">
   <div class="elementoTime">
     <div class="tempo _duration_selector">
       <p>New time</p>
       <input class="numberTime" maxlength="<%= t('language_parameters.video_editor.length_duration') %>"/>
       <p class="lower"><%= t('captions.seconds_shortened') %></p>
     </div>
   </div>
    <a class="done _media_player_done_other_component_in_video_editor_preview upper"><%= t('captions.done') %></a>
  </div>
  <p style="padding-left:10px"><%= t('captions.time') %>: 10 sec</p>
</div>
