<div id="video_editor_preview_container" class="contentVideo">
  <% cont = 1 %>
  <% parameters[:components].each do |c| %>
    <%= render :partial => "video_editor/previews/#{c[:type]}", :locals => {:component => c, :identifier => cont, :empty => false} %>
    <% cont += 1 %>
  <% end %>
  <% if parameters[:audio_track] %>
    <%= render :partial => 'video_editor/previews/audio_track', :locals => {:audio_track => parameters[:audio_track]} %>
  <% end %>
</div>
