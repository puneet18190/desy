$('#media_element_report_form_<%= @media_element_id %>').hide();
var obj = $('#report_media_element_click_<%= @media_element_id %>');
obj.removeClass('<%= Buttons::REPORT %>_light');
obj.addClass('<%= Buttons::REPORT %>');

<% if @resp %>
  showOkPopUp('<%= j t("other_popup_messages.reports.ok") %>');
<% else %>
  showErrorPopUp('<%= j @error %>');
<% end %>
