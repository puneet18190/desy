<div class="header">
  <div class="contenitore">
  <h1 class="fltlft upper"><%= t('image_editor.label') %></h1>
    <div id="image_editor_title" class="testo fltlft">
      <% if (image.nil? || image.is_public) %>
        <span class="_untitled">
          <%= t('media_elements_editor.new_untitled') %>
        </span>
      <% else %>
        <span class="_titled">
          <%= image.title %>
        </span>
        <span class="_untitled" style="display:none">
          <%= t('media_elements_editor.new_untitled') %>
        </span>
      <% end %>
    </div>
    <%= link_to '', my_media_elements_path, :class => 'esciButton _exit_url', :title => t('general_titles.exit_title') %>
    <script type="text/javascript" charset="utf-8">
      if('<%= @back %>'.length > 0) {
        var bb = $('.esciButton');
        bb.attr('href', '<%= @back %>');
        bb.show();
      }
    </script>
  </div>
</div>
