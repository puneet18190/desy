if(<%=@crop%>){
  $("#cropped_image").imgAreaSelect({ 
    hide: true
  });
  $("._toggle_crop").removeClass("current");
  $("#image_wrapper img").attr("src","/media_elements/images/<%=@image_id%>/editing/user_<%=@current_user.id%>/<%= @custom_filename %>");
  $('input[name="cropped_image"]').val("<%= @custom_filename %>");
  resetSelect();
}