<% if @ok %>
  
  <% @lessons.each do |l| %>
    $('#virtual_classroom_quick_select_container form').append('<%= j(render(:partial => "virtual_classroom/view_single_in_quick_load", :locals => {:lesson => l})) %>');
  <% end %>
  
  $('#virtual_classroom_quick_select_container').data('page', <%= @page %>);
  
  initializeNotAvailableLessonsToLoadQuick();
  
<% end %>
