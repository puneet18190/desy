console.log("<%=@parent%>");
<% if @parent.sti_type == 'Province' %>
  $('#town_id').selectbox("detach");
  $('#user_location_id').selectbox("detach");
  $('select#town_id').html("<%= j render :partial => 'users/locations_list', :locals => {:parent => @parent, :selected => nil} %>")
  $('select#user_location_id').html("");
  $('#town_id').selectbox();
  $('#user_location_id').selectbox();
<% end %>

<% if @parent.sti_type == 'Town' %>
  $('#user_location_id').selectbox("detach");
  $('select#user_location_id').html("<%= j render :partial => 'users/locations_list', :locals => {:parent => @parent, :selected => nil} %>")
  $('#user_location_id').selectbox();
<% end %>
