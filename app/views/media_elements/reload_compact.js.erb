<% if @ok %>
  <% js_content = j(render(:partial => "media_elements/view_single_compact", :locals => {:media_element => @media_element, :destination => @destination})) %>
  var target = $('#<%= @destination %>_<%= @media_element_id %>');
  target.replaceWith("<%= js_content %>");
<% else %>
  showErrorPopUp('<%= @error %>', 'Error!!');
<% end %>
