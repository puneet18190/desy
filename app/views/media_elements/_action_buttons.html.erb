<div class="menuController" id="media_element_menu_buttons_<%= media_element.id %>">
  <% cont = 0 %>
  <% media_element.buttons.each do |b| %>
    <%= render :partial => "shared/buttons/#{b}", :locals => {:item => media_element, :destination => destination} %>
    <% cont += 1 %>
  <% end %>
  <% while cont < 6 %>
    <%= image_tag 'hidden.png' %>
    <% cont += 1 %>
  <% end %>
</div>
