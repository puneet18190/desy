<div class="gray-bar">
  <%= render :partial => "media_elements/graybar_left" %>
  <%= render :partial => "shared/search" %>
</div>
<br class="clearfloat">
<div class="griglia-contenuti">
  <div class="elements-content">
    <%= render :partial => 'media_elements/title_bar' %>
    <br class="clearfloat">
    <%= render :partial => "media_elements/my_media_elements" %>
    <div id="info_container" style="display:none" data-currenturl="<%= request.url %>"></div>
    <br class="clearfloat">
    <% if @media_elements.any? %>
      <%= render :partial => 'shared/pagination', :locals => {:page => @page, :pages_amount => @pages_amount, :pagination_url => manipulate_request_url(remove_query_param: 'delete_item') } %>
    <% end %>
  </div>
</div>

<%= render partial: 'media_elements/new_form_popup' %>
