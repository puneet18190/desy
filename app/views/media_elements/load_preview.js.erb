<% if @ok %>
  $('#dialog-media-element-<%= @media_element_id %>').data('loaded', true);
  $('#dialog-media-element-<%= @media_element_id %>').html($('<%= j render(:partial => "media_elements/popup", :locals => {:media_element => @media_element, :locked => false}) %>').html());
  showMediaElementInfoPopUp(<%= @media_element_id %>);
<% end %>
