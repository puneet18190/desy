<script>
  window.onload = function() {
    <% if @errors %>
      top.uploaderErrorsMediaElements(<%= @errors.to_json.html_safe %>, <%= @error_fields.to_json.html_safe %>);
    <% else %>
      top.uploadRedirect('load-media-element');
    <% end %>
    top.uploadDone('load-media-element');
  }
</script>
