<!DOCTYPE html>
<html class="application-layout <%= controller_html_class %> <%= action_html_class %>">
<head>
  <title>Desy</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render :partial => "shared/popups" %>
<%= render :partial => "shared/loading" %>
<div class="container">
  <%= render :partial => "shared/header" %>
  <div class="content">
  <%= render :partial => "shared/header_content_elements/graybar", :locals => {:where => @where} %>
  <br class="clearfloat">
  <div class="griglia-contenuti">
  
  
    <%= yield %>
    
  </div>
  <br class="clearfloat">
  </div>
  <%= render :partial => "shared/footer" %>
</div>
</body>
</html>
