<div class="row-fluid">
  <div class="span12">
    <h3><%= t('admin.media_elements.edit') %></h3>
    <div id="new_elements_preview" class="well">
      <div class="row-fluid">
        <% @private_elements.each_with_index do |element,index| %>
        <% if index % 4 == 0 %>
          </div>
          <div class="row-fluid">
        <% end %>
          <%= render :partial => 'element_update', :locals => {:element => element, :colspan => 3} %>
        <% end %>
      </div>
      <div style="clear:both">
    </div>
  </div>
</div>
