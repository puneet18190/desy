<div class="audioEditorTimeline">
  <div id="audio_editor_box_ghost" class="boxGhost" style="display:none;"></div>
  <div id="audio_editor_timeline">
      <% cont = 1 %>
      <% components.each do |p| %>
        <%= render :partial => "audio_editor/component", :locals => {:component => p, :identifier => cont, :empty => false} %>
        <% cont += 1 %>
      <% end %>
  </div>
</div>
