  <metadata>
    <schema>ADL SCORM</schema>
    <schemaversion>2004 3rd Edition</schemaversion>
    <lom>
      <general>
        <identifier>
          <catalog>URI</catalog>
          <entry><%= lesson_viewer_url @lesson.id %></entry>
        </identifier>
        <title>
          <string language="<%= scorm_locale %>"><%= @lesson.title %></string>
        </title>
        <language><%= scorm_locale %></language>
        <description>
          <string language="<%= scorm_locale %>"><%= @lesson.description %></string>
        </description>
        <% @lesson.tags.split(',').each do |t| %>
          <keyword>
            <string language="<%= scorm_locale %>"><%= t %></string>
          </keyword>
        <% end %>
        <coverage>
          <string language="en">Lesson about <%= @lesson.subject.description %></string>
        </coverage>
      </general>
      <lifeCycle>
        <status>
          <source>LOMv1.0</source>
          <value>final</value>
        </status>
        <%= scorm_author @lesson.user.full_name, @lesson.updated_at %>
      </lifeCycle>
      <%= scorm_metametadata %>
      <technical>
        <%= scorm_requirements %>
      </technical>
      <educational>
        <interactivityType>
          <source>LOMv1.0</source>
          <value>expositive</value>
        </interactivityType>
        <interactivityLevel>
          <source>LOMv1.0</source>
          <value>low</value>
        </interactivityLevel>
        <intendedEndUserRole>
          <source>LOMv1.0</source>
          <value>learner</value>
        </intendedEndUserRole>
        <context>
          <source>LOMv1.0</source>
          <value>school</value> <!-- qui devo mappare lo school_level in qualche modo -->
        </context>
        <language>it</language> <!-- locale -->
      </educational>
      <rights>
        <copyrightAndOtherRestrictions>
          <source>LOMv1.0</source>
          <value>yes</value>
        </copyrightAndOtherRestrictions>
        <description>
          <string language="en">©2013 DESY. For information about copyright contact Morgan S.P.A. via degli Olmetti 36</string> <!-- inserire copyright e riempirlo -->
        </description>
      </rights>
    </lom>
